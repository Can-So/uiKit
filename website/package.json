    "Jed W",
    "Trey S"
  ],
  "scripts": {
    "webpack:dev": "./node_modules/@atlaskit/webpack-config/bin/dev.js ./src/index.js",
    "webpack:build": "./node_modules/@atlaskit/webpack-config/bin/build.js ./src/index.js",
    "build": "yarn build:webpack && yarn build:copy && yarn build:sandbox && yarn build:minimize && yarn build:zip",
    "build:no-minimize": "yarn build:webpack && yarn build:copy && yarn build:zip",
    "build:webpack": "rm -rf dist && yarn run webpack:build",
    "build:copy": "cp dist/index.html dist/404.html && cp -R public/* dist/",
    "build:sandbox": "node ./build/sandbox",
    "build:zip": "rm -rf dist.zip && zip -r dist.zip dist/",
    "build:minimize": "find dist -name '*.js' -type f -size +100k -prune -exec sh -c  \"echo Minimizing: {} && ./node_modules/.bin/uglifyjs --compress hoist_funs=false '{}' -o '{}'\" \\;",
    "start": "yarn webpack:dev",
    "deploy": "yarn build && netlify-patched -t $NETLIFY_TOKEN deploy --site-id $NETLIFY_SITE_ID -p dist.zip",
    "deploy-staging": "yarn build && netlify-patched -t $NETLIFY_TOKEN deploy --site-id $NETLIFY_SITE_ID_STAGING -p dist.zip",
    "deploy-staging:no-minimize": "yarn build:no-minimize && netlify-patched -t $NETLIFY_TOKEN deploy --site-id $NETLIFY_SITE_ID_STAGING -p dist.zip"
  },
  "repository": "atlaskit/fabric",
  "devDependencies": {
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "bolt-query": "^1.1.4",
    "chalk": "^2.1.0",
    "extract-react-types-loader": "^0.0.0",
    "netlify-cli": "^1.2.2"
  },
  "dependencies": {
    "@atlaskit/badge": "^6.1.6",
    "@atlaskit/button": "^6.1.0",
    "@atlaskit/code": "^2.1.9",
    "@atlaskit/css-reset": "^1.1.5",
    "@atlaskit/dynamic-table": "^6.0.2",
    "@atlaskit/field-text": "^4.0.0",
    "@atlaskit/icon": "^10.3.1",
    "@atlaskit/layer-manager": "^2.5.6",
    "@atlaskit/logo": "^6.0.9",
    "@atlaskit/lozenge": "^3.4.8",
    "@atlaskit/media-card": "^23.0.3",
    "@atlaskit/media-core": "^12.0.2",
    "@atlaskit/media-filmstrip": "^5.7.6",
    "@atlaskit/media-test-helpers": "^8.5.1",
    "@atlaskit/navigation": "^26.5.1",
    "@atlaskit/single-select": "^3.0.1",
    "@atlaskit/tag": "^3.0.13",
    "@atlaskit/tag-group": "^3.4.11",
    "@atlaskit/theme": "^2.2.9",
    "@atlaskit/webpack-config": "^1.0.0",
    "@lbatchelor/netlify": "^1.2.0-patched.7",
    "@lbatchelor/netlify-cli": "^1.2.2-patched.7",
    "babel-runtime": "^6.26.0",
    "commonmark": "^0.27.0",
    "commonmark-react-renderer": "^4.3.3",
    "connect-history-api-fallback": "^1.5.0",
    "html-webpack-plugin": "^2.30.1",
    "minimatch": "^3.0.4",
    "prismjs": "^1.6.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-ga": "^2.3.5",
    "react-loadable": "^5.1.0",
    "react-markdown": "^2.5.0",
