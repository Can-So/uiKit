{
  "presets": [["env", { "modules": false }], "react", "flow"],
  "plugins": ["transform-class-properties", "transform-object-rest-spread", "syntax-dynamic-import"],
  "env": {
    "production": {
      "presets": ["env", "react", "flow"],
      "plugins": ["transform-class-properties", "transform-object-rest-spread", "transform-runtime"]
    },
    "test": {
      "presets": ["env", "react", "flow"],
      "plugins": ["transform-class-properties", "transform-object-rest-spread"]
    }
  }
}
