    "traverse.es.js",
    "traverse.es.d.ts",
    "traverse.js",
    "traverse.d.ts",
    "validator.es.js",
    "validator.es.d.ts",
    "validator.js",
    "validator.d.ts"
  ],
  "author": "Atlassian Pty Ltd",
  "maintainers": [
    "Stanislav S"
  ],
  "dependencies": {
    "@atlaskit/editor-common": "^20.1.4"
  },
  "devDependencies": {
    "@atlaskit/docs": "^5.0.7",
    "@atlaskit/json-schema-generator": "^1.2.0"
  },
  "repository": "https://bitbucket.org/atlassian/atlaskit-mk-2"
