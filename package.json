{
  "name": "@atlaskit/fabric",
  "private": true,
  "version": "0.0.0",
  "description": "Fabric components for Atlassian's Atlaskit",
  "main": "index.js",
  "repository": "atlaskit/fabric",
  "author": "Atlassian Pty Ltd",
  "license": "Apache-2.0",
  "scripts": {
    "build": "                    run-p build:babel build:typescript --print-label --continue-on-error",
    "build:babel": "              NODE_ENV=production bolt workspaces exec --only-fs 'packages/{elements,utils}/**' -- babel src -d dist/cjs",
    "build:typescript": "         NODE_ENV=production bolt workspaces exec --only-fs 'packages/fabric/*' -- tsc",
    "fml": "                      rm -rf ./**/node_modules && rm -rf */*/*/node_modules && rm -rf */*/node_modules",
    "test": "                     yarn run test:jest",
    "test:jest": "                jest --coverage",
    "test:browser": "             projector ./projector test:browser",
    "test:browser:browserstack": "projector ./projector test:browser --browserstack",
    "lint": "                     run-p lint:eslint lint:tslint --continue-on-error",
    "lint:eslint": "              eslint '**/*.js'",
    "lint:tslint": "              tslint -c ./tslint.json 'packages/*/*/src/**/*.+(ts|tsx)' --format stylish",
    "typecheck": "                run-p typecheck:flow typecheck:typescript --aggregate-output --continue-on-error",
    "typecheck:flow": "           flow check",
    "typecheck:typescript": "     tsc --noEmit",
    "website:start": "            cd website && yarn start",
    "start": "                    yarn run website:start",
    "changeset": "                projector ./projector changeset",
    "release": "                  projector ./projector release"
  },
  "bolt": {
    "version": "0.15.x",
    "workspaces": [
      "build/*",
      "packages/utils/*",
      "packages/elements/*",
      "packages/fabric/*",
      "patterns",
      "website"
    ]
  },
  "devDependencies": {
    "@atlaskit/eslint-config-base": "^2.0.7",
    "@atlaskit/layer": "^2.5.4",
    "@types/chai": "^4.0.4",
