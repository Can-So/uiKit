    "uuid": "^3.1.0"
  },
  "engines": {
    "node": "^8.4.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "./node_modules"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest/preprocessor",
      "^.+\\.js$": "babel-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.json",
        "skipBabel": true
      }
    },
    "testRegex": "(/(__tests?__/.*)\\.?(test|spec)?\\.(js|tsx?))$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ]
  }
}
