    ],
    "resolver": "./resolver.js",
    "transform": {
      "^.+\\.tsx?$": "ts-jest/preprocessor",
      "^.+\\.js$": "babel-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.fabric.json",
        "skipBabel": true
      }
    },
    "testRegex": "(/(__tests?__/)([^_].*/)*?[^_][^/]*?\\.?(test|spec)?\\.(js|tsx?))$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
