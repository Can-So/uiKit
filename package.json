  "scripts": {
    "build":
      "                               run-p build:pkg build:babel && run-p build:typescript:es5 build:typescript:es2015 build:editor-common:json-schema --print-label --continue-on-error",
    "build:pkg":
      "                           bolt workspaces exec --only-fs 'packages/**' -- copy-pkg package.json dist/package.json --only name,version",
    "build:babel":
      "                         NODE_ENV=production bolt workspaces exec --only-fs 'packages/{elements,utils}/**' -- babel src -d dist/cjs",
    "build:typescript:es5":
      "                NODE_ENV=production bolt workspaces exec --only-fs 'packages/fabric/*' -- tsc --project ./build/es5",
    "build:typescript:es2015":
      "             NODE_ENV=production bolt workspaces exec --only-fs 'packages/fabric/*' -- tsc --project ./build/es2015",
    "delete": "                              yarn run delete:modules && yarn run delete:build",
    "delete:modules": "                      rm -rf node_modules **/node_modules",
    "delete:build": "                        rm -rf packages/*/*/dist website/dist",
    "precommit": "                           lint-staged",
    "test": "                                yarn run test:jest",
    "test:jest": "                           jest --coverage",
    "test:browser": "                        projector ./projector test:browser",
    "test:browser:watch": "                  projector ./projector test:browser --watch",
    "test:browser:browserstack": "           projector ./projector test:browser --browserstack",
    "lint": "                                yarn run lint:eslint && yarn run lint:tslint",
    "lint:eslint": "                         eslint '**/*.js'",
    "lint:tslint":
      "                         tslint-config-prettier-check ./tslint.json && tslint -c ./tslint.json 'packages/*/*/src/**/*.+(ts|tsx)' --format stylish",
    "typecheck": "                           yarn run typecheck:flow && yarn run typecheck:typescript",
    "typecheck:flow": "                      flow check",
    "typecheck:typescript": "                bolt workspaces exec --only-fs 'packages/fabric/*' -- tsc --noEmit --project ./",
    "website:start": "                       cd website && yarn start",
    "start": "                               yarn run website:start",
    "start:elements": "                      WEBSITE=elements yarn run website:start",
    "start:editor": "                        WEBSITE=editor   yarn run website:start",
    "changeset": "                           projector ./projector changeset",
    "release": "                             projector ./projector release",
    "build:editor-common:json-schema": "     cd packages/fabric/editor-common && yarn run build:json-schema && echo 'Done.'"
  },
  "bolt": {
    "version": "0.15.x",
    "workspaces": ["build/*", "packages/utils/*", "packages/elements/*", "packages/fabric/*", "patterns", "website"]
